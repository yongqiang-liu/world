{
  "productName": "worldh5", // 世界H5日常处理器
  "appId": "net.seven.shijieh5",
  "copyright": "Copyright © 2022 Seven",
  "asar": true,
  "files": ["dist"],
  "win": {
    "target": [
      {
        "target": "nsis",
        "arch": "x64"
      }
      // {
      //   target: "nsis",
      //   arch: "ia32",
      // },
    ]
  },
  "icon": "./assets/icons/win/icon.ico",
